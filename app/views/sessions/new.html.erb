
<%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %><br>
<%= link_to "Sign in with Facebook", "/auth/facebook", id: "sign_in" %>
<br><br>
<%= form_for(:user, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password %><br>
  <%= f.submit 'Login' %>
<% end %>